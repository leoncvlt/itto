{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/build/web/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": ["app.*"],
  "modes": {
    "window": {
      "title": "zero",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/src/icon.png"
    }
  },
  "cli": {
    "binaryName": "zero",
    "resourcesPath": "/build/web/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/src/neutralino.js",
    "binaryVersion": "4.4.0",
    "clientVersion": "3.3.0"
  }
}
