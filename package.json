{
  "name": "itto",
  "private": true,
  "version": "0.0.0",
  "main": "./dist/itto.core.js.js",
  "module": "./dist/itto.core.js.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/itto.core.js",
    "./utils": "./dist/itto.utils.js"
  },
  "scripts": {
